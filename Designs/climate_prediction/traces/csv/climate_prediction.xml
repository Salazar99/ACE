<harm>
    <context name="climate_prediction_test">


        <!-- <prop exp="temperature >= -10 && temperature <= 10 && pressure >= 950 && pressure <= 1000" loc="dt"/>
        <prop exp="temperature >= 11 && temperature <= 25 && pressure >= 1000 && pressure <= 1020" loc="dt"/>
        <prop exp="temperature > 25 && pressure < 950" loc="dt"/>
        <prop exp="!(temperature >= -10 && temperature <= 10 && pressure >= 950 && pressure <= 1000) &&
                   !(temperature >= 11 && temperature <= 25 && pressure >= 1000 && pressure <= 1020) &&
                   !(temperature > 25 && pressure < 950)" loc="dt"/> -->


        <prop exp="intf::snow" loc="c"/>
        <prop exp="intf::sunny" loc="c"/>
        <prop exp="intf::storm" loc="c"/>
        <prop exp="intf::error" loc="c"/>

        <!-- 0E to remove 0 values, or the clustering will be fail -->
        <numeric clustering="K,0E,30Max,0.01WCSS,><,>=,<=,==" exp="DUT::reg_temperature" loc="dt"/>
        <numeric clustering="K,0E,30Max,0.01WCSS,><,>=,<=,==" exp="DUT::reg_pressure" loc="dt"/>
        <prop exp="intf::done" loc="dt"/>

        <template dtLimits="8A,6D,6W,-0.1E" exp="G({..&&..} |-> P0)" /> 
        <!--<template dtLimits="8A,6D,6W,-0.1E" exp="G(P0 && P1 && P2 |-> P3)" />-->

        <!-- <template check="1" exp="G(temperature > 25 && pressure < 950 && DU == 1 |-> {storm == 1})" /> -->
        <!-- <template check="1" exp="G((temperature >= -10 && temperature <= 10) && (pressure >= 950 && pressure <= 1000) && DU == 1 |-> {snow == 1})" /> -->
        
        <!-- <template check="1" exp="G(DUT::reg_temperature > 25 && DUT::reg_pressure < 950 && intf::done == 1 |-> {intf::storm == 1})" /> -->

        <sort name="ATCT-AFCT" exp="atct-afct"/>

    </context>
</harm>