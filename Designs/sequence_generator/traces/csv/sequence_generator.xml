<harm>
    <context name="sequence_generator_test">
        
		<prop exp="start_value >= range_min" loc="a"/>
		<prop exp="start_value <= range_max" loc="a"/>
		<prop exp="step > 0" loc="a"/>
		<prop exp="step < 0" loc="a"/>
        <numeric clustering="K,5Max,0.01WCSS,>=,==" exp="sequence_value" loc="c"/>
        <numeric clustering="K,5Max,0.01WCSS,>=,==" exp="start_value + step" loc="[dt]"/>
        
        <template dtLimits="4A,3D,2D,-0.1E,U,O" exp="G(P0 -> P1)" />
		<template dtLimits="4A,3D,2D,-0.1E,U,O" exp="G(P0 && P1 -> P2)" />
		<template dtLimits="5D,3W,15A,-0.1E,U" exp="G({..#1&..}|-> P0)" /> 
        
        <filter name="causality" exp="1-afct/traceLength" threshold="0.2"/>
        <sort name="frequency" exp="atct/traceLength"/>

    </context>
</harm>