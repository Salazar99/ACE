<harm>
	<context name="default">

		<prop exp="in > 0" loc="a, dt"/>
		<prop exp="in < 0" loc="a, dt"/>
		<prop exp="in == 0" loc="a, dt"/>
		<prop exp="start" loc="a, dt"/>

		<prop exp="out_DU" loc="c"/>
		<prop exp="error_DU" loc="c"/>
		<prop exp="done_DU" loc="c"/>
		<prop exp="!out_DU" loc="c"/>
		<prop exp="!error_DU" loc="c"/>
		<prop exp="!done_DU" loc="c"/>


		<template dtLimits="2D,2W,4A,0E,U" exp="G(P0 |-> ##12 P1)" />
		<template dtLimits="2D,2W,4A,0E,U" exp="G(P2 |-> ##1 P3)" />
		<template dtLimits="2D,2W,4A,0E,U" exp="G(P4 |-> ##12 P5 && P6)" />
		<template dtLimits="2D,2W,4A,0E,U" exp="G(P7 |-> ##1 P8 && P9)" />
		<template dtLimits="2D,2W,4A,0E,U" exp="G(P10 && P11 |-> ##12 P12 && P13 && P14)" />

		<template check="1" exp="G((in > 0) |-> ##12 out_DU)" />
		<template check="1" exp="G((in < 0) |-> ##1 error_DU)" />
		<template check="1" exp="G((in == 0) && start |-> ##12 !out_DU && !error && done_DU)" />

		<sort name="1/(1+afct)" exp="1/(1+afct)"/>
		<sort name="ATCT-AFCT" exp="atct-afct"/>
	</context>
</harm>



