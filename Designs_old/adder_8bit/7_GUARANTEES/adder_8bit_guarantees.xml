<harm>
	<context name="default">

		<prop exp="a" loc="a, dt"/>
		<prop exp="b" loc="a, dt"/>
		<prop exp="cin" loc="a, dt"/>
		<prop exp="a + b + cin > 255" loc="a, dt"/>
		<prop exp="a + b + cin <= 255" loc="a, dt"/>
		<prop exp="a + b + cin == 0" loc="a, dt"/>
		<prop exp="a + b + cin == 256" loc="a, dt"/>


		<prop exp="sum_DU" loc="c"/>
		<prop exp="cout_DU" loc="c"/>
		<prop exp="!sum_DU" loc="c"/>
		<prop exp="!cout_DU" loc="c"/>

		<!--<template dtLimits="2D,2W,4A,0E,U" exp="G({..#1&..}|-> X(camallia_u::EncDec && X[23]camallia_u::Dvld))" /> 
		<template dtLimits="2D,2W,4A,0E,U" exp="G({..#1&..}|-> X(!camallia_u::EncDec && X[23]camallia_u::Dvld))" /> -->

		<template exp="G(P0 |-> P2)" />
		<template exp="G(P3 |-> P4 && P5)" />
		<template dtLimits="3D,3W,4A,0E,U" exp="G(..&&.. |-> P1)" />
		<template dtLimits="3D,3W,4A,0E,U" exp="G(..#1&.. |-> P2)" />
		<template dtLimits="3A,-0.1E,U" exp="G({..##1..}|-> X(P0))" /> 

		<!-- 
		<template check="1" exp="G((a + b + cin) > 255 |-> cout_DU)" />
		<template check="1" exp="G((a + b + cin) == 0 |-> sum_DU == 0 && cout_DU == 0)" />
		<template check="1" exp="G((a + b + cin) == 256 |-> sum_DU == 0 && cout_DU == 1)" />
		<template check="1" exp="G((a + b + cin) == 256 |-> sum_DU == 0)" />-->
		

		<!-- <sort name="ATCT-AFCT" exp="atct - afct"/>
		<sort name="ATCT-AFCT" exp="atct / (atct + afct)"/>-->
		<sort name="1/(1+afct)" exp="1 / (1 + afct)"/>
		<sort name="ATCT-AFCT" exp="atct / (atct + afct)"/>
	</context>
</harm>



