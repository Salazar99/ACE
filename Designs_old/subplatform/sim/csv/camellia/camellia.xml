<harm>
	<context name="default">

		<prop exp="camallia_u::Krdy" loc="a, dt"/>
		<prop exp="camallia_u::Drdy" loc="a, dt"/>
		<prop exp="camallia_u::EncDec" loc="a, dt"/>

		<prop exp="camallia_u::RSTn" loc="c"/>
		<prop exp="camallia_u::EncDec" loc="c"/>
		<prop exp="!camallia_u::EncDec" loc="c"/>
		<prop exp="camallia_u::Dvld" loc="c"/>
		<prop exp="camallia_u::Kvld" loc="c"/>



		<template dtLimits="2D,2W,4A,0E,U" exp="G({..#1&..}|-> X(camallia_u::EncDec && X[23]camallia_u::Dvld))" /> 
		<template dtLimits="2D,2W,4A,0E,U" exp="G({..#1&..}|-> X(!camallia_u::EncDec && X[23]camallia_u::Dvld))" /> 

		<template check="1" exp="G((!(camallia_u::Drdy) ##1 (camallia_u::Drdy))  |-> ##24 camallia_u::Dvld)" />
		<template check="1" exp="G((!(camallia_u::Drdy) ##1 (camallia_u::Drdy) ##1 (camallia_u::Drdy)) |-> ##23 camallia_u::Dvld)" />
		<template check="1" exp="G(!(camallia_u::Krdy) ##1 (camallia_u::Krdy) |-> nexttime[7](camallia_u::Kvld))" />
		<template check="1" exp="G(!(camallia_u::Krdy) ##1 (camallia_u::Krdy) |-> X(X(X(X(X(X(X(camallia_u::Kvld))))))))" />
		<template check="1" exp="G(!(camallia_u::Drdy && camallia_u::EncDec) ##1 (camallia_u::Drdy && camallia_u::EncDec) |-> nexttime[1]((camallia_u::EncDec and nexttime[23](camallia_u::Dvld))))" />
		<template check="1" exp="G(!(camallia_u::Drdy && !camallia_u::EncDec) ##1 (camallia_u::Drdy && !camallia_u::EncDec) |-> nexttime[1]((!camallia_u::EncDec and nexttime[23](camallia_u::Dvld))))" />
		

		<sort name="ATCT-AFCT" exp="atct-afct"/>
	</context>
</harm>



