<harm>
	<context name="default">

		<prop exp="A > B" loc="a, dt"/>
		<prop exp="A < B" loc="a, dt"/>
		<prop exp="A == B" loc="a, dt"/>
		<prop exp="A >= B" loc="a, dt"/>
		<prop exp="A <= B" loc="a, dt"/>
		<prop exp="A != B" loc="a, dt"/>
		<prop exp="A" loc="a, dt"/>
		<prop exp="B" loc="a, dt"/>

		<prop exp="A_greater" loc="c"/>
		<prop exp="A_equal" loc="c"/>
		<prop exp="A_less" loc="c"/>



		<!--<template dtLimits="2D,2W,4A,0E,U" exp="G({..#1&..}|-> X(camallia_u::EncDec && X[23]camallia_u::Dvld))" /> 
		<template dtLimits="2D,2W,4A,0E,U" exp="G({..#1&..}|-> X(!camallia_u::EncDec && X[23]camallia_u::Dvld))" /> -->


		<template dtLimits="2D,2W,4A,0E,U" exp="G(..&&.. |-> P1)" />
		<template dtLimits="2D,2W,4A,0E,U" exp="G(..#1&.. |-> P2)" />
		<template dtLimits="3A,-0.1E,U" exp="G({..##1..}|-> X(P0))" /> 
		<template dtLimits="2D,2W,4A,0E,U" exp="G(P3 |-> P4)" />

		<sort name="frequency" exp="atct/traceLength"/>
		<filter name="ATCT-AFCT" exp="(atct-afct)/(atct+afct)" threshold = "0"/>
		<sort name="1/(1+afct)" exp="1/(1+afct)"/>
  		<sort name="ATCT-AFCT" exp="(atct-afct)/(atct+afct)"/>

	</context>
</harm>



